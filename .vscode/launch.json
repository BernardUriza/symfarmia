{
  "WillLuke.nextjs.addTypesOnSave": true,
  "WillLuke.nextjs.hasPrompted": true,
  "terminal.integrated.enablePersistentSessions": true,
  "terminal.integrated.persistentSessionReviveProcess": "onExitAndWindowClose",
  "restoreTerminals.showOnStartup": true,
  "restoreTerminals.showOnStartupMessage": "Restoring terminals from last session",
  "restoreTerminals.showOnStartupMessageTimeout": 5000,
  "restoreTerminals.terminals": [
    {
      "splitTerminals": [
        {
          "name": "run dev",
          "commands": ["npm run dev"],
          "shouldRunCommands": false
        },
        {
          "name": "claude",
          "commands": ["claude"]
        },
        {
          "name": "monitor-live",
          "commands": ["ccusage blocks --live"]
        }
      ]
    }
  ]
}
