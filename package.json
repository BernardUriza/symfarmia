{
  "name": "symfarmia-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "validate:translations": "node scripts/validate-translations.js",
    "validate:translations:revolutionary": "node scripts/revolutionary-translation-validator.js",
    "test:translations": "jest tests/medical-components/translations.test.js",
    "dev": "node scripts/kill-ports.js && concurrently \"node scripts/build-guardian.js --dev && NODE_OPTIONS='--max-old-space-size=4096 --no-warnings' next dev --turbo\" \"npm run dev:susurro\"",
    "kill:ports": "node scripts/kill-ports.js",
    "dev:brave": "npm run kill:brave-cache && npm run dev",
    "dev:clean": "rm -rf .next && npm run kill:brave-cache && npm run dev",
    "kill:brave-cache": "node scripts/kill-brave-cache.js",
    "validate-turbo": "node scripts/validate-turbo.js",
    "build": "npm run version:generate && npx prisma generate && next build",
    "build:guardian": "node scripts/build-guardian.js --build && npm run build",
    "build:enhanced": "node scripts/enhanced-build.js",
    "build:emergency": "node scripts/build-recovery.js emergency",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=1024' WEBPACK_MINIMIZE=false next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "version:generate": "node scripts/generate-version.js",
    "permanent:start": "node scripts/permanent-dev-simple.js start",
    "permanent:stop": "node scripts/permanent-dev-simple.js stop",
    "permanent:restart": "node scripts/permanent-dev-simple.js restart",
    "permanent:status": "node scripts/permanent-dev-simple.js status",
    "permanent:logs": "node scripts/permanent-dev-simple.js logs",
    "dev:susurro": "cd microservices/susurro-test && npm run dev",
    "setup:susurro": "cd microservices/susurro-test && npm run setup",
    "health:susurro": "curl -f http://localhost:3001/api/health",
    "setup:microservices": "./scripts/setup-microservices.sh",
    "test:microservice": "node scripts/microservice-e2e-guardian.js",
    "test:microservice:susurro": "cd microservices/susurro-test && npm test"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.8.0",
    "@edgestore/react": "0.2.2",
    "@edgestore/server": "0.2.2",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@headlessui/react": "1.7.19",
    "@heroicons/react": "2.0.18",
    "@material-tailwind/react": "2.1.10",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tremor/react": "^3.18.7",
    "alertifyjs": "1.14.0",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "16.6.1",
    "file-saver": "2.0.5",
    "framer-motion": "10.18.0",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "^0.525.0",
    "next": "^15.3.5",
    "next-pwa": "^5.6.0",
    "node-fetch": "3.3.2",
    "nodejs-whisper": "^0.2.9",
    "nodemailer": "6.10.1",
    "openai": "^5.8.2",
    "pdf-lib": "1.17.1",
    "postcss": "8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "2.5.2",
    "react-icons": "^5.5.0",
    "react-is": "^19.1.0",
    "react-loader-spinner": "6.1.6",
    "react-slick": "0.29.0",
    "recharts": "^3.1.0",
    "resend": "2.1.0",
    "slick-carousel": "1.8.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.4.17",
    "wavefile": "^11.0.0",
    "web-audio-api": "^0.2.2",
    "zod": "3.25.67",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@faker-js/faker": "9.8.0",
    "@headlessui/tailwindcss": "0.2.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.6.1",
    "@types/file-saver": "2.0.7",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "20.19.2",
    "@types/nodemailer": "6.4.17",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/react-slick": "0.23.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^7.6.0",
    "critters": "^0.0.23",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.30",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^6.11.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
