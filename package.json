{
  "name": "symfarmia-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=1024 --no-warnings' next dev",
    "dev:turbo": "NODE_OPTIONS='--max-old-space-size=4096' next dev --turbopack",
    "build": "npm run version:generate && npm run type-check && npx prisma generate && next build",
    "build:enhanced": "node scripts/enhanced-build.js",
    "build:emergency": "node scripts/emergency-build.js",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=1024' WEBPACK_MINIMIZE=false next build --no-lint",
    "start": "next start",
    "lint": "next lint && npm run type-check",
    "lint:fix": "next lint --fix",
    "lint:medical": "node scripts/lint-medical.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:type-check": "npm run type-check && npm run test",
    "test:medical-modules": "jest tests/medical-modules.test.js tests/medical-workflows.test.js tests/performance.test.js",
    "test:medical": "npm run test:medical-modules && npm run test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "deploy": "npm run version:generate && npm run build",
    "accessibility:check": "node scripts/check-accessibility.js",
    "accessibility:test": "npm run test tests/accessibility/",
    "version:generate": "node scripts/generate-version.js",
    "version:bump": "npm version patch && npm run version:generate",
    "translations:ensure": "node scripts/ensure-translations.js",
    "analyze": "ANALYZE=true npm run build",
    "postinstall": "npx prisma generate",
    "auto-build": "./scripts/auto-build.sh",
    "auto-build:medical": "./scripts/auto-build.sh --medical-critical",
    "auto-build:production": "./scripts/auto-build.sh --production",
    "build:analyze": "ANALYZE=true npm run build",
    "validate:routes": "node scripts/validate-medical-routes.js",
    "check:hipaa": "node scripts/check-hipaa.js",
    "cache:clean": "rm -rf .next node_modules/.cache dist .turbo coverage && npm cache clean --force",
    "ci:local": "npm run cache:clean && npm ci && npm run auto-build",
    "ci:medical": "npm run cache:clean && npm ci && npm run auto-build:medical",
    "ci:status": "./scripts/concise-cicd-system.sh status",
    "ci:restart": "./scripts/concise-cicd-system.sh restart",
    "ci:stop": "./scripts/concise-cicd-system.sh stop",
    "ci:logs": "./scripts/concise-cicd-system.sh logs"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.8.0",
    "@edgestore/react": "0.2.2",
    "@edgestore/server": "0.2.2",
    "@headlessui/react": "1.7.19",
    "@heroicons/react": "2.0.18",
    "@material-tailwind/react": "2.1.10",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tremor/react": "^3.18.7",
    "alertifyjs": "1.14.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "2.30.0",
    "dotenv": "16.6.1",
    "file-saver": "2.0.5",
    "framer-motion": "10.18.0",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "^0.525.0",
    "next": "^14.2.25",
    "node-fetch": "3.3.2",
    "nodemailer": "6.10.1",
    "openai": "^5.8.2",
    "pdf-lib": "1.17.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "2.5.2",
    "react-icons": "^5.5.0",
    "react-loader-spinner": "6.1.6",
    "react-slick": "0.29.0",
    "resend": "2.1.0",
    "slick-carousel": "1.8.1",
    "tailwind-merge": "^3.3.1",
    "zod": "3.25.67",
    "zustand": "^5.0.6",
    "next-pwa": "^6.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "9.8.0",
    "@headlessui/tailwindcss": "0.2.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.6.1",
    "@types/file-saver": "2.0.7",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "20.19.2",
    "@types/nodemailer": "6.4.17",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/react-slick": "0.23.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "10.4.21",
    "critters": "^0.0.23",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.30",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.5.6",
    "prettier": "^3.2.5",
    "prisma": "^6.11.1",
    "tailwindcss": "3.4.17",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  }
}
