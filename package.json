{
  "name": "symfarmia-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run type-check && npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint && npm run type-check",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:type-check": "npm run type-check && npm run test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "deploy": "node generate-date.js && git-commit.bat",
    "analyze": "ANALYZE=true npm run build",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "3.8.0",
    "@edgestore/react": "0.2.2",
    "@edgestore/server": "0.2.2",
    "@headlessui/react": "1.7.19",
    "@heroicons/react": "2.0.18",
    "@material-tailwind/react": "2.1.10",
    "@prisma/client": "5.22.0",
    "@tremor/react": "^3.18.7",
    "alertifyjs": "1.14.0",
    "date-fns": "2.30.0",
    "dotenv": "16.6.1",
    "file-saver": "2.0.5",
    "framer-motion": "10.18.0",
    "jsonwebtoken": "9.0.2",
    "next": "14.2.30",
    "node-fetch": "3.3.2",
    "nodemailer": "6.10.1",
    "pdf-lib": "1.17.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "2.5.2",
    "react-icons": "4.12.0",
    "react-loader-spinner": "6.1.6",
    "react-slick": "0.29.0",
    "resend": "2.1.0",
    "slick-carousel": "1.8.1",
    "zod": "3.25.67",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@faker-js/faker": "9.8.0",
    "@headlessui/tailwindcss": "0.2.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.6.1",
    "@types/file-saver": "2.0.7",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "20.19.2",
    "@types/nodemailer": "6.4.17",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/react-slick": "0.23.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "10.4.21",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.30",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.5.6",
    "prettier": "^3.2.5",
    "prisma": "5.22.0",
    "tailwindcss": "3.4.17",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  }
