# Environment Variables for SYMFARMIA
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_MAX_TOKENS=4000

# Auth0 Configuration
AUTH0_SECRET=your_auth0_secret_here
AUTH0_BASE_URL=http://localhost:3000
AUTH0_ISSUER_BASE_URL=https://your-auth0-domain.auth0.com
AUTH0_CLIENT_ID=your_auth0_client_id_here
AUTH0_CLIENT_SECRET=your_auth0_client_secret_here

# Database Configuration
DATABASE_URL=your_database_url_here

# EdgeStore Configuration
EDGE_STORE_ACCESS_KEY=your_edge_store_access_key_here
EDGE_STORE_SECRET_KEY=your_edge_store_secret_key_here

# Medical AI Configuration
MEDICAL_AI_API_KEY=your_medical_ai_api_key_here

# Development Settings
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1

# Cache and Performance
NEXT_PRIVATE_SKIP_VALIDATE_EXPERIMENTAL_CACHE=true